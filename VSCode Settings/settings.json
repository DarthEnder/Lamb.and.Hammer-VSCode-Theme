// Place your settings in this file to overwrite the default settings
{
    "git.enabled": true,
    "csharp.suppressDotnetInstallWarning": true,
    "editor.codeLens": false,
    "workbench.iconTheme": "vscode-great-icons",
    "workbench.activityBar.visible": true,
    "workbench.sideBar.location": "left",
    "workbench.colorTheme": "Lamb&Hammer HC",
    "git.enableSmartCommit": true,
    "editor.minimap.enabled": false,
    "editor.renderIndentGuides": false,
    "editor.fontSize": 12,
    "editor.cursorStyle": "line",
    "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
    "editor.lineHeight": 20,
    "editor.letterSpacing": 0,
    "editor.lineNumbers": "relative",
    "editor.overviewRulerBorder": true,
    "editor.renderWhitespace": "none",
    "editor.renderControlCharacters": false,
    "editor.renderLineHighlight": "gutter",
    "window.restoreFullscreen": true,
    "workbench.colorCustomizations": {
        "contrastBorder": "#666666",
        "contrastActiveBorder": "#666666",
        "tab.border": "#666666",
        "editorGroup.border": "#666666",
        "panel.border": "#666666",
        "editor.lineHighlightBorder": "#666666",
        "statusBar.border": "#666666",
        "editor.foreground": "#EFEFEF",
        "editor.background": "#000000",
        "panel.background": "#000000",
        "sideBar.background": "#000000",
        "activityBar.background": "#000000",
        "statusBar.background": "#000000",
        "tab.activeBackground": "#000000",
        "tab.inactiveBackground": "#000000",
        "titleBar.activeBackground": "#000000",
        "dropdown.background": "#999999",
        "tab.unfocusedInactiveBackground": "#000000",
        "tab.unfocusedInactiveForeground": "#999999",
        "tab.activeForeground": "#EFEFEF",
        "editorGroupHeader.noTabsBackground": "#000000",
        "titleBar.inactiveBackground": "#000000",
        "editorGroupHeader.tabsBackground": "#000000",
        "badge.background": "#000000",
        "list.dropBackground": "#000000",
        "list.focusBackground": "#000000",
        "input.background": "#000000",
        "list.hoverBackground": "#000000",
        "editorGutter.background": "#000000",
        "editorGroup.dropBackground": "#000000",
        "notification.background": "#000000",
        "editorHoverWidget.background": "#000000"
    }
}